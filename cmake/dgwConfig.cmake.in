@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Required dependencies
find_dependency(Eigen3 3.4)

# Optional dependencies
if(@DGW_ENABLE_OPENMP@)
    find_dependency(OpenMP)
endif()

if(@DGW_ENABLE_PETSC@)
    find_dependency(PkgConfig)
    pkg_check_modules(PETSC PETSc)
endif()

if(@netCDF_FOUND@)
    find_dependency(netCDF)
endif()

include("${CMAKE_CURRENT_LIST_DIR}/dgwTargets.cmake")

check_required_components(dgw)
